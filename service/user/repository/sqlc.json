{
  "version": "1",
  "packages": [
    {
      "path": "mysql",
      "name": "mysql",
      "schema": "mysql/schema",
      "queries": "mysql/query",
      "engine": "mysql",
      "emit_json_tags": true,
      "emit_prepared_queries": true,
      "emit_interface": true,
      "overrides": [
        {
          "column": "user.id",
          "go_type": "github.com/google/uuid.UUID"
        }
      ]
    },
    {
      "path": "postgresql",
      "name": "postgresql",
      "schema": "postgresql/schema",
      "queries": "postgresql/query",
      "engine": "postgresql",
      "emit_json_tags": true,
      "emit_prepared_queries": true,
      "emit_interface": true
    }
  ]

}